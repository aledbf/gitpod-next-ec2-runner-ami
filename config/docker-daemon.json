{
    "debug": false,
    "bip": "172.17.0.1/16",
    "experimental": true,
    "max-concurrent-downloads": 10,
    "max-concurrent-uploads": 10,
    "max-download-attempts": 10,
    "live-restore": false,
    "default-shm-size": "128M",
    "exec-opts": [
        "native.cgroupdriver=systemd"
    ],
    "dns-opts": [
        "timeout:5"
    ],
    "features": {
        "containerd-snapshotter": true
    }
}
